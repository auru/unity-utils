{
  "name": "unity-utils",
  "version": "2.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run test && ./node_modules/.bin/babel src --out-dir .",
    "lint": "./node_modules/.bin/eslint --ignore-path=.gitignore --fix ./src",
    "lint-prod": "NODE_ENV='production' npm run lint",
    "prepublish": "npm run build",
    "precommit-msg": "echo 'Running pre-commit checks...' && exit 0",
    "test": "npm run lint-prod && ./node_modules/.bin/ava --verbose",
    "test-watch": "npm run lint && ./node_modules/.bin/ava --verbose --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auru/unity-utils.git"
  },
  "author": "TimeRaider (vitaliy@blinovskov.com)",
  "contributors": [{
    "name": "Anton Konev",
    "email": "info@antonkonev.com"
  }],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "homepage": "https://github.com/auru/unity-utils#readme",
  "bugs": {
    "url": "https://github.com/auru/unity-utils/issues"
  },
  "keywords": [
    "unity",
    "util",
    "utils",
    "utility",
    "utilities"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "path": "^0.12.7"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.16.3",
    "eslint": "^3.7.1",
    "eslint-plugin-ava": "^3.1.0",
    "pre-commit": "^1.1.3"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js"
    ],
    "source": [
      "src/*.js"
    ],
    "concurrency": 4,
    "failFast": false,
    "tap": false,
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
